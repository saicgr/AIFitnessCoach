<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>FitWiz</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>FitWiz</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIRequiresFullScreen</key>
	<false/>
	<key>UISupportsMultipleScenes</key>
	<true/>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSHealthShareUsageDescription</key>
	<string>FitWiz needs access to your health data to sync measurements like weight, body fat, and heart rate from Apple Health.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>FitWiz needs access to save your workout data and body measurements to Apple Health.</string>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>instagram-stories</string>
		<string>instagram</string>
	</array>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>FitWiz needs access to save workout recap images to your photo library.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>FitWiz needs access to the microphone for voice-to-text food logging.</string>
	<key>NSCameraUsageDescription</key>
	<string>FitWiz needs camera access to take food photos for AI nutrition analysis and barcode scanning.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>FitWiz needs your location to find nearby gyms and set up automatic gym profile switching.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>FitWiz uses background location to automatically switch to your gym profile when you arrive at a saved gym location.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>FitWiz uses background location to automatically switch to your gym profile when you arrive at a saved gym location.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>FitWiz uses Bluetooth to connect to your heart rate monitor and display live heart rate during workouts.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>fetch</string>
		<string>remote-notification</string>
		<string>bluetooth-central</string>
	</array>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>com.fitwiz.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>fitwiz</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
